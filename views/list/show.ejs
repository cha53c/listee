<!DOCTYPE html>
<html lang="en">
<%- include('list/head') %>
<body>
<script> const userId = '<%- userId %>'; const listId = '<%= listId %>' ; const listname = '<%- listName %>' </script>
<div class="container-fluid bg-warning">
    <h1>List name: <%= listName %>
        <span><%- include('list/editSaveButtons', { editAction: 'showEditAction()',
            saveAction: 'showSaveAction(userId, listId, listname)', cancelAction: 'cancelAction()'}) %></span>
        <span><i id="delete-list-btn" class="update-page hide material-icons-two-tone md-36" type="button" onclick="deleteList(userId, listId)" data-toggle="tooltip" title="delete list">delete</i></span>
    </h1>
</div>
<div class="container bg-light" >
<a href="/lists/<%= userId %>">Back to my lists</a>
<h2>List items</h2>
<%- include('list/listItems', { items: items, addButton: true, withLinks: false }) %>
</div>
</body>
<%- include('bootstrapScripts') %>
<script src="/static/listFunctions.js"></script>
</body>
</html>