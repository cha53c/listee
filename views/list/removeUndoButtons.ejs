<i id="del<%= id %>" class="remove-btn hide" data-itemId="<%= id %>"
   onclick="<%= removeAction %>" data-toggle="tooltip" title="delete item">delete</i>
<i id="undo<%= id %>" class="undo-btn hide" data-itemId="<%= id %>"
   onclick="<%= undoAction %>" data-toggle="tooltip" title="undo delete">undo</i>
